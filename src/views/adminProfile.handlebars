            </div>
        </div>
        <div class="tab-content" id="users">{{> adminUsers}}</div>
        <div class="tab-content" id="doctors">{{> adminDoctors}}</div>
        <div class="tab-content" id="nurses">{{> adminNurses}}</div>
        <div class="tab-content" id="bookings">{{> adminBookings}}</div>
